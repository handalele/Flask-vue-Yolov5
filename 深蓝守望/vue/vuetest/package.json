    "echarts": "^5.5.0",
    "element-plus": "^2.7.0",
    "socket.io-client": "^4.7.5",
    "vue": "^3.2.13",
    "vue-baidu-map-3x": "^1.0.38",
    "vue-baidu-map-offline": "^1.0.7",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0",
    "vue3-mini-weather": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
