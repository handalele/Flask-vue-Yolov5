      "no-console": "off",
      "no-irregular-whitespace": "off",
      "no-debugger": "off",
      "multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
